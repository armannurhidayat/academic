<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<data>

		<record id="session_sequence" model="ir.sequence">
			<field name="name">Session squence</field>
			<field name="code">academic.session</field>
			<field name="prefix">Sess/</field> <!-- Code unik di awal -->
			<field name="suffix">/sion</field> <!-- Code unik di akhir -->
			<field name="padding">3</field> <!-- Panjang code unik -->
			<field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
		</record>

		<record id="course_sequence" model="ir.sequence">
			<field name="name">Course squence</field>
			<field name="code">academic.course</field>
			<field name="prefix">Course/</field>
			<field name="suffix">/sion</field>
			<field name="padding">3</field>
			<field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
		</record>

	</data>
</odoo>
